{
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/services",
      "destination": "/services.html"
    },
    {
      "source": "/about",
      "destination": "/about.html"
    },
    {
      "source": "/contact",
      "destination": "/contact.html"
    },
    {
      "source": "/team",
      "destination": "/team.html"
    },
    {
      "source": "/track-shipment",
      "destination": "/track-shipment.html"
    },
    {
      "source": "/privacy-policy",
      "destination": "/privacy-policy.html"
    },
    {
      "source": "/terms-of-service",
      "destination": "/terms-of-service.html"
    },
    {
      "source": "/cookie-policy",
      "destination": "/cookie-policy.html"
    },
    {
      "source": "/get-quote",
      "destination": "/get-quote.html"
    },
    {
      "source": "/admin-login",
      "destination": "/admin/login.html"
    },
    {
      "source": "/admin",
      "destination": "/admin/shipments.html"
    },
    {
      "source": "/admin/shipments",
      "destination": "/admin/shipments.html"
    },
    {
      "source": "/admin/create",
      "destination": "/admin/create.html"
    },
    {
      "source": "/admin/shipment-details",
      "destination": "/admin/shipment-details.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}

